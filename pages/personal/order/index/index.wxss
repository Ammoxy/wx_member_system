/* 排序 */
.rank {
    display: flex;
    justify-content: space-around;
    padding: 10rpx 0;
    background-color: #fff;
}

/* .list {
    height: 88vh;
} */

.item {
    display: flex;
    flex-direction: column;
    margin: 20rpx;
    padding: 20rpx;
    border-radius: 10rpx;
    box-shadow: 5rpx 5rpx 5rpx #ccc;
    background-color: #fff;
}

.item .state {
    align-self: flex-end;
    padding: 10rpx 0;
}


.goods {
    display: flex;
}

.item image {
    width: 200rpx;
    height: 200rpx;
}

.item .info {
    margin-left: 20rpx;
}

.item .info .name {
    width: 460rpx;
    font-size: 36rpx;
    font-weight: 700;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}

.price-box {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.price-box .text {
    color: #999;
    font-size: 24rpx;
    margin-right: 20rpx;
}

.price-box .price {
    font-size: 32rpx;
    font-weight: bold;
    color: #444;
}

.item .handle {
    display: flex;
    justify-content: flex-end;
    margin-top: 10rpx;
}

.item .handle .button {
    margin-left: 10rpx;
    padding: 10rpx 20rpx;
    border-radius: 30rpx;
    font-size: 30rpx;
    color: #fff;
}

.item .time {
    color: #CC0033;
    margin-top: 20rpx;
    align-self: flex-end;
}

.choose {
    border-bottom: 4rpx solid #CC0033;
    color: #CC0033;
}

/* 医师列表 */
.doctorBox {
    width: 100vw;
    padding: 20rpx 0;
    display: flex;
    font-size: 30rpx;
}

.doctorBox .doctorList {
    position: relative;
    width: 28vw;
    min-width: 28vw;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.doctorBox .doctorList .portrait {
    width: 25vw;
    height: 25vw;
}

.doctorBox .doctorList view {
    width: 25vw;
    height: 40rpx;
    display: flex;
    justify-content: center;
}

.doctorBox .doctorList text {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    text-overflow: ellipsis;
    overflow: hidden;
}

.doctorBox .doctorList .name {
    color: #333;
    /* font-weight: bold; */
}

.doctorBox .doctorList .post {
    color: #333;
    font-size: 26rpx;
}

.doctorBox .doctorList .good {
    font-size: 26rpx;
}

.doctorBox .doctorList .lable {
    width: 70rpx;
    min-width: 70rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    border-radius: 10rpx;
    background-color: #ccc;
    color: #333;
    margin-right: 10rpx;
}